@model SelectList




<blockquote class="blockquote-reverse">
    @if(Model.Count()!=0)
    { 
    <div class="row">
        <div><h4 class="text-muted">Мои маршруты: @Html.ActionLink("Редактировать", "Edit", "Routes", null, new { @class = "text-right" })</h4></div>
        <div class="col-lg-12">
            @Html.DropDownList("userRoutes", (Model), "", new { @class = "form-control", onchange = "SelectRoute();" })
        </div>
    </div>
    <div id="Routes">

    </div>
    }
    else
    {
        <h4>@Html.ActionLink("Добавьте", "Add", "Routes") свои наиболее часто используемые маршруты для более удобного просмотра расписания</h4>
    }
</blockquote>
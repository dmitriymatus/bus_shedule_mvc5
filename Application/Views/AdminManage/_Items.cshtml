@model IQueryable<Application.Models.ApplicationUser>

@foreach (var item in Model)
{
    <li class="list-group-item"><h5 class="text-info"><span class="glyphicon glyphicon-user" aria-hidden="true"></span> @Html.ActionLink(item.UserName, "User", "AdminManage", new { id = item.Id }, null)</h5></li>
}
